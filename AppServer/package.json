{
  "name": "Continuum",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "pod-install": "cd ios && pod install",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "debug-electron": "react-scripts build && cp -r platform/electron/electron.js ./electron.js && electron ./electron.js && rm electron.js && rm -r build",
    "release-electron": "react-scripts build && cp -r platform/electron/electron.js build && electron-builder --dir && rm -r build"
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "^0.2.9",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/compat": "^5.3.20",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/stack": "^6.3.17",
    "@use-expo/font": "^2.0.0",
    "electron": "^25.3.0",
    "electron-builder": "^24.4.0",
    "expo": "^49.0.3",
    "expo-app-loading": "^2.1.1",
    "expo-asset": "^8.10.1",
    "expo-font": "^11.4.0",
    "expo-modules-core": "^1.5.7",
    "galio-framework": "^0.8.0",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-native": "0.72.3",
    "react-native-bundle-loader": "^0.1.0",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-modal-dropdown": "^1.0.2",
    "react-native-reanimated": "^3.3.0",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.22.1",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.9",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "build": {
    "appId": "org.reactjs.native.example.continuum",
    "mac": {
      "category": "org.reactjs.native.example.continuum.mac"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
