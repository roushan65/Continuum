{
  "private": true,
  "name": "@continuum/workbench",
  "version": "0.0.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@theia/core": "latest",
    "@theia/filesystem": "latest",
    "@theia/markers": "latest",
    "@theia/messages": "latest",
    "@theia/monaco": "latest",
    "@theia/navigator": "latest",
    "@theia/preferences": "latest",
    "@theia/process": "latest",
    "@theia/workspace": "latest",
    "@continuum/core": "*",
    "@continuum/workflow-editor-extension": "*"
  },
  "devDependencies": {
    "@theia/cli": "latest",
    "monaco-editor-webpack-plugin": "^7.1.0"
  },
  "scripts": {
    "build": "theia build --mode=development",
    "build:prd": "theia build --mode=production",
    "build:dev": "theia build --mode=development",
    "rebuild": "theia rebuild:browser --cacheRoot ..",
    "start": "theia start --plugins=local-dir:./plugins --port 3000",
    "watch": "yarn rebuild && theia build --watch --mode development"
  },
  "theia": {
    "target": "browser"
  }
}
